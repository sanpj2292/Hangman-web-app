  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sanpj2292/Hangman-web-app/issues"
  },
  "homepage": "https://github.com/sanpj2292/Hangman-web-app#readme",
  "devDependencies": {
    "nodemon": "^2.0.3"
  },
  "dependencies": {
    "concurrently": "^5.3.0"
  }
}
